{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "attaquer-modified",
  "version": "0.3.0",
  "description": "A modified version of attaquer theme (https://github.com/iAttaquer/.glzr).",
  "tags": [
    "simple"
  ],
  "previewImages": [
    "resources\\preview-image.png"
  ],
  "repositoryUrl": "https://github.com/Emptope/.glzr",
  "widgets": [
    {
      "name": "top-bar",
      "htmlPath": "top-bar/dist/index.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "top-bar/dist/**"
      ],
      "caching": {
        "defaultDuration": 31536000,
        "rules": [
          {
            "pattern": "**/*.{js,css}",
            "duration": 31536000
          },
          {
            "pattern": "**/*.{ttf,woff,woff2,eot,otf}",
            "duration": 31536000
          },
          {
            "pattern": "**/*.{png,jpg,jpeg,gif,svg,ico}",
            "duration": 31536000
          }
        ]
      },
      "privileges": {
        "shellCommands": [
          {
            "program": "explorer.exe",
            "argsRegex": "ms-actioncenter://",
            "urlRegex": ""
          },
          {
            "program": "C:\\Program Files\\SystemInformer\\SystemInformer.exe",
            "argsRegex": "",
            "urlRegex": ""
          },
          {
            "program": "C:\\Program Files\\Mem Reduct\\memreduct.exe",
            "argsRegex": "",
            "urlRegex": ""
          }
        ]
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "40px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
